<ServerManagerConfiguration>
  <ProxyGroup name="filters">
   <SourceProxy name="DiscreteMarchingCubes" class="vtkDiscreteMarchingCubes" label="DiscreteMarchingCubes">
     <Documentation
        long_help=""
        short_help="">
     </Documentation>
     <InputProperty
         name="DiscreteMarchingCubes"
	 port_index="0"
         command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkImageData"/>
        </DataTypeDomain>
        <Documentation>
          Specify the input vtkImageData.
        </Documentation>
      </InputProperty>
 

      <IntVectorProperty
	  name="ComputeAdjacentScalars"
	  command="SetComputeAdjacentScalars"
	  number_of_elements="1"
	  default_values="0">
	<BooleanDomain name="bool"/>
      </IntVectorProperty>

      <IntVectorProperty
	  name="ComputeScalars"
	  command="SetComputeScalars"
	  number_of_elements="1"
	  default_values="1">
	<BooleanDomain name="bool"/>
      </IntVectorProperty>

      <IntVectorProperty
	  name="GenerateValues"
	  command="GenerateValues"
	  number_of_elements="3"
	  animateable="1"
	  default_values="1 1 1">
      </IntVectorProperty>

   </SourceProxy>
 </ProxyGroup>
</ServerManagerConfiguration>


